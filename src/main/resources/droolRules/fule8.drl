//保质期外
rule "store_rule_13"
    when $s: StoreRule(orderType==3,guaranteeStatus == 2,obuSelect == 0,etcChannel >=1 && etcChannel <=8)
    then
        $s.setAfterSaleCard(-1);
        $s.setAfterSaleObu(-1);
